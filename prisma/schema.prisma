  team_id_away           BigInt?
  team_abbreviation_away String?   @db.VarChar(10)
  team_city_name_away    String?   @db.VarChar(255)
  team_nickname_away     String?   @db.VarChar(255)
  team_wins_losses_away  String?   @db.VarChar(10)
  pts_qtr1_away          Decimal?  @db.Decimal
  pts_qtr2_away          Decimal?  @db.Decimal
  pts_qtr3_away          Decimal?  @db.Decimal
  pts_qtr4_away          Decimal?  @db.Decimal
  pts_ot1_away           Decimal?  @db.Decimal
